{
  "name": "nodejs-profi",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=development npm run watch ./src/bin/www",
    "prod": "npm run build && NODE_ENV=production node ./dist/bin/www",
    "build": "npm run clean && babel ./src --config-file ./.babelrc --out-dir dist --copy-files --source-maps inline",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "bigint-hash": "^0.2.2",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "helmet": "^3.21.2",
    "morgan": "~1.9.0",
    "portfinder": "^1.0.25",
    "pug": "^3.0.1",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.4"
  }
}
